								"option 1",
								"option 2",
								"option 3"
							],
							"addOther": true,
							"required": true
						},
						{
							"type": "multipart-question",
							"conditional": "true",
							"parts": {
								"1": {
									"question": {
										"title": "Will the participants be recorded?",
										"format": "radio",
										"options": [
											"Yes",
											"No"
										]
									},
									"branches": [
										{
											"part": "2",
											"value": "Yes"	
										},
										{
											"part": "4",
											"value": "Yes"
										}
									]
								},
								"2": {
									"question": {
										"title": "Will the recordings be",
										"format": "checkbox",
										"options": [
											"Video",
											"Audio"
										],
										"required": true
									},
									"branches": [
										{
											"part": "3",
											"value": "Video"
										}
									]
								},
								"3": {
									"question": {
										"title": "Please state what will be recorded - participants face, or just hands/gestures",
										"format": "input",
										"required": true
									},
									"branches": []
								},
								"4": {
									"question": {
										"title": "Why is the recording required?",
										"subtitle": "Recording must be destroyed after transcription, please state this",
										"format": "textbox",
										"required": true
									},
									"branches": []
								}
							},
							"required": true
						}
					]
				},
				{
					"type": "section",
					"title": "Section 2",
					"description": "This is section 2",
					"elements": [
						{
							"type": "checkbox-group",
							"title": "Does your research involve any of the following?",
							"defaultBranch": {
								"type": "replacement",
								"replacements": [
									{
										"expedited-body": "application-2:full-body"
									}
								]
							},
							"checkboxes": [
								{
									"title": "Participants under 18?"
								},
								{
									"title": "People with an illness or relatives of a person with an illnes?"
								},
								{
									"title": "Patients in a GP or hospital (or HSE staff)?",
									"branch": {
										"type": "action",
										"action": "terminate",
										"reason": "The ethics committee cannot approve research involving patients of a GP or hospital, or HSE staff. COntact HSE's research ethics committee"
									}
								}
							]
						}
					]
				},
				{
					"type": "container",
					"id": "expedited-body",
					"sections": [
					]
				}
			]
