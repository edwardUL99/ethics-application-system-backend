{
	"applications": {
		"expedited": {
			"id": 1,
			"name": "Ethics Application Form",
			"description": "Form description here",
			"version": "1.0",
			"components": [
				{
					"type": "section",
					"title": "Section 1",
					"description": "Section description here",
